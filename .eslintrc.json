{
   "env": {
      "es6": true,
      "node": true
   },
   "extends": "airbnb-base",
   "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
   },
   "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
   },
   "rules": {
      "indent": ["error", 3],
      "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
      "no-param-reassign": 0,
      "no-console": "off"
   }
}